FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    
    inlet
    {
        type         	 fixedValue;
        value		 uniform (1.1 0 0);
    }

    outlet
    {
        type            zeroGradient;  
    }

    atmosphere
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }

	riverSlope_riverSlope
	{
		type            fixedValue;
		value		uniform (0 0 0);
	}


	left
	{
		type           	slip;
	}


	right
	{
		type           	slip;
	}



    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
