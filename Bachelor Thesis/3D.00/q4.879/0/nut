FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
   
    inlet
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;  
    }

    atmosphere
    {
        type            zeroGradient;
    }

	riverSlope_riverSlope
	{
        		type            nutSpalartAllmarasWallFunction;
        		Cmu             0.09;
        		kappa           0.41;
        		E               9.8;
        		Cs              uniform 0.5;
        		Ks              uniform 0.001;
        		value           uniform 0.01; 
	}


	left
	{
        type            zeroGradient;
	}


	right
	{
        type            zeroGradient;
	}
}

