FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

  	inlet
 	{
        	type            fixedValue;
       		value           uniform 0;
    	}
	outlet
    	{
        	type            zeroGradient;
    	}
 	atmosphere
    	{
        	type            inletOutlet;
        	inletValue      uniform 0;
        	value           uniform 0;
    	}

    	riverSlope_riverSlope
	{
       		type 		fixedValue;
		value		uniform 1;
	}


	left
	{
		type 		     slip;
	
	}


	right
	{
		type 		     slip;
	}

    defaultFaces
    {
        type            empty;
    }
}

