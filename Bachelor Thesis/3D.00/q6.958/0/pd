FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    inlet
    {
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 0;
    }


    outlet
    {
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 0;
    }


    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1;
        value           uniform 0;
    }

	riverSlope_riverSlope
	{
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 0;
	}

 
	left
	{
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 0;
	}


	right
	{
        type            buoyantPressure;
        gradient        uniform 0;
        value           uniform 0;
	}

}
	
