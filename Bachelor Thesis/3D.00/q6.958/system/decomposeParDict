{
    n               ( 4 2 2 );
    delta           0.001;
}

hierarchicalCoeffs 
{
    n               ( 1 1 1 );
    delta           0.001;
    order           xyz;
}

metisCoeffs
{
    processorWeights ( 1 1 1 1 );
}

manualCoeffs
{
    dataFile        "";
}

