FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          HU;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -1 0 0 0 0];

internalField   uniform (0.625 0 0);

boundaryField
{
    inlet
    {
        type       	flowRateInlet;
        flowRate    	0.0625;
        value           uniform (0 0 0);
    }

    outlet
    {
       type 	  zeroGradient;
    }

    right
    {
        type       empty;
    }

    left
    {
        type       empty;
    }

    atmosphere
    {
        type       empty;
    }
    bottomWall
    {
        type       empty;
    }

}


