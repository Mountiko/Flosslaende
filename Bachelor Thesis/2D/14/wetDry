FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "14";
    object      wetDry;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1;
    }
    outlet
    {
        type            calculated;
        value           uniform 1;
    }
    right
    {
        type            empty;
    }
    left
    {
        type            empty;
    }
    atmosphere
    {
        type            empty;
    }
    bottomWall
    {
        type            empty;
    }
}


